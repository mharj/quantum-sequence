{
    "name": "quantum-sequence",
    "version": "0.8.1",
    "description": "Map and Set implementations which uses tachyon-drive as storage backend",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --sourcemap --format cjs,esm --dts --clean",
        "prepublishOnly": "npm run build",
        "test": "nyc mocha",
        "coverage": "nyc report --reporter=lcovonly",
        "lint": "eslint src test",
        "validate": "tsc --noEmit --project tsconfig.test.json"
    },
    "files": [
        "dist"
    ],
    "repository": "github:mharj/quantum-sequence",
    "keywords": [
        "storage",
        "driver",
        "tachyon-drive",
        "iterator"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "author": "mharj",
    "license": "MIT",
    "devDependencies": {
        "@stylistic/eslint-plugin": "^2.1.0",
        "@tsconfig/node16": "^16.1.3",
        "@types/chai": "^4.3.16",
        "@types/chai-as-promised": "^7.1.8",
        "@types/mocha": "^10.0.6",
        "@types/node": "^18.19.34",
        "@types/sinon": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^7.12.0",
        "@typescript-eslint/parser": "^7.12.0",
        "chai": "^4.4.1",
        "chai-as-promised": "^7.1.2",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-sonarjs": "^0.23.0",
        "mocha": "^10.4.0",
        "nyc": "^15.1.0",
        "prettier": "^3.3.0",
        "sinon": "^18.0.0",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.3",
        "tsup": "^8.1.0",
        "typescript": "^5.4.5",
        "zod": "^3.23.8"
    },
    "dependencies": {
        "@avanio/logger-like": "^0.2.0",
        "tachyon-drive": "^0.8.0"
    }
}
