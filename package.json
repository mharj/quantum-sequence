{
    "name": "quantum-sequence",
    "version": "0.1.0",
    "description": "Map and Set implementations which uses tachyon-drive as storage backend",
    "main": "./dist/index.js",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "nyc mocha",
        "coverage": "nyc report --reporter=lcovonly",
        "lint": "eslint src"
    },
    "files": [
        "dist"
    ],
    "repository": "github:mharj/quantum-sequence",
    "keywords": [
        "storage",
        "driver",
        "tachyon-drive",
        "iterator"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "author": "mharj",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.3.11",
        "@types/chai-as-promised": "^7.1.8",
        "@types/mocha": "^10.0.6",
        "@types/node": "^14.18.63",
        "@types/sinon": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "chai": "^4.4.1",
        "chai-as-promised": "^7.1.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-deprecation": "^2.0.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-sonarjs": "^0.23.0",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "prettier": "^3.2.4",
        "sinon": "^17.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "zod": "^3.22.4"
    },
    "dependencies": {
        "@avanio/logger-like": "^0.1.1",
        "tachyon-drive": "^0.3.4"
    }
}
